{
  "name": "kerplunk-twilio",
  "displayName": "Twilio",
  "description": "Text yo self before you wreck yo self",
  "version": "0.0.1",
  "kerplunk": {
    "permissions": {
      "kerplunk-identity": {
        "models": [
          "Identity"
        ]
      }
    }
  },
  "keywords": [
    "kerplunk-plugin",
    "kp:Utility",
    "kp:Notifications"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/twilio.js",
  "dependencies": {
    "lodash": "^3.10.0",
    "react": "^0.13.3",
    "twilio": "^2.3.0",
    "when": "^3.7.3"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "glut": "0.0.1",
    "glut-coffee-amdify": "0.0.0",
    "gulp": "^3.8.8",
    "gulp-coffee": "^2.3.1"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib ./public && gulp"
  }
}
